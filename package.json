{
  "name": "freestyle",
  "version": "0.0.1",
  "description": "simple freestyle setup",
  "author": "zanuka",
  "main": "index.js",
  "private": true,
  "scripts": {
    "ci": "node test.js && zuul -- test.js",
    "test": "node test.js | faucet && zuul --local 9966 -- test.js",
    "coverage": "istanbul cover --report none --print detail test/index.js",
    "view-coverage": "istanbul report html && open ./coverage/index.html",
    "travis": "npm run coverage -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanuka/freestyle.git"
  },
  "bugs": {
    "url": "https://github.com/zanuka/freestyle/issues"
  },
  "keywords": [
    "javascript",
    "test",
    "tape",
    "zuul"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zanuka/freestyle#readme",
  "devDependencies": {
    "coveralls": "^2.11.4",
    "eslint": "^1.2.0",
    "eslint-config-xo": "^0.3.1",
    "faucet": "0.0.1",
    "istanbul": "^0.3.20",
    "istanbul-coveralls": "^1.0.3",
    "tape": "^4.2.0",
    "zuul": "^3.4.0"
  },
  "dependencies": {}
}
