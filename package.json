{
  "name": "originator",
  "version": "0.0.3",
  "description": "node module originator",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zanuka/originator.git"
  },
  "author": {
    "name": "zanuka",
    "email": "git@zanuka.com",
    "url": "https://github.com/zanuka"
  },
  "scripts": {
    "ci": "babel-node test/*.test.js && zuul -- test/*.test.js",
    "clean": "rm -rf lib",
    "compile": "babel --stage 0 -d lib/ src/",
    "test": "eslint src/*.js && babel-node test/*.test.js | tspec",
    "coverage": "istanbul cover babel-node test/*.test.js",
    "coveralls": "cat ./coverage/istanbul/lcov.info | coveralls && rm -rf ./coverage",
    "zuul": "babel-node test/*.test.js && zuul --local 9966 -- test/*.test.js",
    "watch": "./node_modules/.bin/babel src -d lib --experimental -w"
  },
  "bugs": {
    "url": "https://github.com/zanuka/originator/issues"
  },
  "keywords": [
    "javascript",
    "module",
    "node",
    "babel",
    "starter",
    "kit",
    "tape",
    "zuul"
  ],
  "homepage": "https://github.com/zanuka/originator#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-istanbul": "^0.3.20",
    "babel-loader": "^5.3.2",
    "babelify": "^6.3.0",
    "blue-tape": "^0.1.10",
    "coveralls": "^2.11.4",
    "eslint": "^1.5.0",
    "eslint-config-xo": "^0.6.0",
    "eslint-plugin-babel": "^2.1.1",
    "isparta": "^3.0.4",
    "istanbul-coveralls": "^1.0.3",
    "tap-spec": "^4.1.0",
    "zuul": "^3.4.0"
  },
  "dependencies": {}
}
