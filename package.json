{
  "name": "originator",
  "version": "0.0.5",
  "description": "node module originator with test & ci pipeline",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zanuka/originator.git"
  },
  "author": {
    "name": "zanuka",
    "url": "https://github.com/zanuka"
  },
  "scripts": {
    "audit": "nsp package",
    "validate": "npm run audit & npm outdated --depth 0",
    "ci": "babel-node test/*.test.js && zuul -- test/*.test.js",
    "clean": "rm -rf lib coverage",
    "compile": "babel --stage 0 -d lib/ src/",
    "test": "babel-node ./node_modules/.bin/blue-tape test/*.test.js | tspec",
    "cover": "npm run compile && babel-node ./node_modules/.bin/istanbul cover ./node_modules/.bin/blue-tape ./lib/*.js --report html --dir ./coverage && npm run clean",
    "coveralls": "npm run compile && babel-node ./node_modules/.bin/istanbul cover ./node_modules/.bin/blue-tape ./lib/*.js --report lcovonly && cat ./coverage/lcov.info | coveralls  && npm run clean",
    "zuul": "babel-tape-runner test/*.test.js && zuul --local 9966 -- test/*.test.js",
    "watch": "./node_modules/.bin/babel src -d lib --experimental -w"
  },
  "bugs": {
    "url": "https://github.com/zanuka/originator/issues"
  },
  "keywords": [
    "javascript",
    "node",
    "module",
    "babel",
    "starter",
    "kit",
    "tape",
    "zuul",
    "istanbul",
    "coveralls",
    "sauce",
    "labs"
  ],
  "homepage": "https://github.com/zanuka/originator#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "blue-tape": "^0.1.10",
    "coveralls": "^2.11.4",
    "eslint": "^1.5.0",
    "eslint-config-xo": "^0.6.0",
    "eslint-plugin-babel": "^2.1.1",
    "istanbul": "^0.3.21",
    "nsp": "^1.1.0",
    "tap-spec": "^4.1.0",
    "zuul": "^3.4.0"
  },
  "dependencies": {}
}
